
        int s = 0;
        int e = x;
        int ans;

        while (s <= e) {
            long long int mid = s + (e - s) / 2;

            if (mid * mid == x) {
                ans = mid;
                return mid;
            }
            if (mid * mid < x) {
                ans = mid;
                s = mid + 1;
            }
            if (mid * mid > x) {
                e = mid - 1;
            }
        }
        return ans;
